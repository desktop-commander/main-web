{
  "name": "main-web",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "cd ./root && npm run build",
    "build:dev": "cd ./root && npm run build:dev",
    "postbuild": "echo \"desktopcommander.app\" > docs/CNAME",
    "commit-static": "git rm -r --cached docs/ 2>/dev/null || true && npm run build && git add docs/ && git commit -m 'Static website changes'",
    "serve": "npx serve docs --listen 3000 --single",
    "serve:static": "npx serve docs --listen 3000 --single",
    "serve:docs": "npx serve docs --listen 3000 --single"
  },
  "repository": {
    "type": "git",
    "url": "git@gh.dc.app:desktop-commander/main-web.git"
  },
